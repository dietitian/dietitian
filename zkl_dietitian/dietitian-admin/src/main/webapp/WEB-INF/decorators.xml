<?xml version="1.0" encoding="utf-8"?>
<decorators defaultdir="/WEB-INF/decorators">
	<!-- 此处用来定义不需要过滤的页面 -->
	<excludes>
		<pattern>/static/*</pattern>
		<pattern>*/login.jsp</pattern>
		<pattern>*/validatecode.jsp</pattern>
	</excludes> 

	<!-- 用来定义装饰器要过滤的页面 -->
	<decorator name="default-template" page="default-template.jsp">
		<pattern>/*</pattern>
	</decorator>
</decorators>
